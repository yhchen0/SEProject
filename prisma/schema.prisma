generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://yanhau:h97881992@localhost:5432/seproject?schema=public"
}

model meeting {
  id           Int         @id @default(autoincrement())
  roomId       room        @map("room_id")
  beginAt      DateTime    @default(now()) @map("begin_at")
  finishAt     DateTime    @default(now()) @map("finish_at")
  // attendees attendees[]
}

model users {
  id           Int         @id @default(autoincrement())
  userName     String      @map("user_name")
  userToken    String      @map("user_token")
  createdAt    DateTime    @default(now()) @map("created_at")
  updatedAt    DateTime    @default(now()) @map("updated_at")
  // attendees attendees[]
}

